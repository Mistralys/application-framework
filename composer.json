{
  "name": "mistralys/application_framework",
  "description": "PHP-based Application Framework",
  "type": "library",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Sebastian Mordziol",
      "email": "s.mordziol@mistralys.eu",
      "homepage": "https://www.mistralys.eu",
      "role": "Lead"
    }
  ],
  "autoload": {
    "classmap": [
      "src/classes/",
      "src/themes/default/templates",
      "tests/classes",
      "tests/application/assets/classes"
    ],
    "files": [
      "src/functions/functions.php"
    ]
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/Mistralys/mousetrap.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/lipis/flag-icon-css.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/medialize/URI.js.git"
    },
    {
      "type": "package",
      "package": {
        "name": "marcj/css-element-queries",
        "version": "1.2.2",
        "type": "library",
        "source": {
          "url": "https://github.com/marcj/css-element-queries.git",
          "type": "git",
          "reference": "master"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "zenorocha/clipboardjs",
        "version": "2.0.8",
        "type": "library",
        "source": {
          "url": "https://github.com/zenorocha/clipboard.js.git",
          "type": "git",
          "reference": "master"
        }
      }
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "mistralys/application-utils": "^1.8.1",
    "mistralys/application-localization": "^1.4.1",
    "mistralys/deepl-xml-translator": "^1.0.5",
    "mistralys/subsetsum": "^1.0.0",
    "mistralys/markdown-viewer": "^1.0.6",
    "pear/http_request2": "^v2.5.0",
    "shark/simple_html_dom": "dev-master",
    "ccampbell/mousetrap": "dev-master",
    "medialize/uri.js": "^1.19.7",
    "ulrichsg/getopt-php": "^4.0.0",
    "components/flag-icon-css": "^3.5.0",
    "desandro/masonry": "v4.2.2",
    "marcj/css-element-queries": "^1.2.2",
    "hybridauth/hybridauth": "^3.7.1",
    "erusev\/parsedown": "^1.7.4",
    "twbs/bootstrap": "^v4.6.0",
    "zenorocha/clipboardjs": "v2.0.8",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-pdo": "*",
    "php": ">=7.4"
  },
  "require-dev": {
    "phpunit/phpunit": ">=9.5",
    "phpstan/phpstan": ">=1.4"
  }
}
